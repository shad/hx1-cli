{
  "name": "hx1-cli",
  "version": "1.0.0",
  "description": "CLI tool for Line 6 HX One guitar effects pedal",
  "main": "dist/index.js",
  "type": "module",
  "bin": {
    "hx1": "./dist/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/shad/hx-one.git"
  },
  "bugs": {
    "url": "https://github.com/shad/hx-one/issues"
  },
  "homepage": "https://github.com/shad/hx-one#readme",
  "scripts": {
    "dev": "bun run src/cli/index.ts",
    "build": "bun build src/cli/index.ts --outdir dist --target bun",
    "build-executable": "bun build src/cli/index.ts --compile --outfile hx1",
    "test": "vitest run --coverage",
    "test:watch": "vitest watch",
    "test:ui": "vitest --ui",
    "check-types": "tsc --noEmit",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "check-all": "bun run check-types && bun run lint && bun run test",
    "complexity": "npx eslint src/**/*.ts --plugin complexity --rule 'complexity: [error, 10]'"
  },
  "keywords": [
    "line6",
    "hx-one",
    "guitar",
    "effects",
    "midi",
    "cli"
  ],
  "author": "Shad Reynolds <shad.reynolds@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "commander": "^12.0.0",
    "chalk": "^5.3.0",
    "easymidi": "^3.1.0",
    "ora": "^8.0.1",
    "inquirer": "^9.2.14"
  },
  "devDependencies": {
    "@types/node": "^20.11.0",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@typescript-eslint/parser": "^6.19.0",
    "@vitest/ui": "^1.2.0",
    "eslint": "^8.56.0",
    "typescript": "^5.3.3",
    "vitest": "^1.2.0",
    "@vitest/coverage-v8": "^1.2.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "bun": ">=1.0.0"
  }
}
